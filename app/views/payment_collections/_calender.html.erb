<div class='w-2/3 m-auto mb-10'>
  <div class='text-amber-dark m-auto'>
    <div id='calender_year'>
      <p><%= Date.today.year %>年度</p>
    </div>
    <div class='flex flex-wrap justify-center text-center'>
      <% (4..12).each do |n| %>
        <div class='w-32'>
          <div id='calender_month' class='border'>
            <%= "#{n}月" %>
          </div>
          <div class='h-20 border'>
            <p>-</p>
          </div>
        </div>
      <% end %>
      <% (1..3).each do |n| %>
        <div class='w-32'>
          <div id='calender_month' class='border'>
            <%= "#{n}月" %>
          </div>
          <div class='h-20 border'>
            <p>-</p>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>